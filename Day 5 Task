h.
  function duplicate(){
var arr = ["one", "two", "three", "four", "two", "three"];
      
    function duplic(arr) {
        return arr.filter((item, 
            index) => arr.indexOf(item) === index);
    }
      
    console.log(duplic(arr));   }
duplicate();
 






