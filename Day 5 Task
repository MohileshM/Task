    function sort()
{

  var array = (arr1, arr2) => [...arr1, ...arr2].sort((a,b) => a-b);

let ar1 =[-7, 2, 4, 22, 66, 99];
let ar2 = [1, 5, 9, 88];

console.log( array(ar1, ar2) ); 
}

sort();

