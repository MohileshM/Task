c.
        function sum(){
        function arr(array) {
  var arr = [1,2,3,4,5];
  let sum = 0;

  for (let i = 0; i < arr.length; i+=1) {
    sum += arr[i];
  }
  
  return sum;
}

console.log(arr([1,2,3,4,5]));
}
sum();

d.
      function prime(){
            var arr = [2, 3, 4, 5, 6, 7, 8, 9, 10]

arr = arr.filter((number) => {
  for (var i = 2; i <= Math.sqrt(number); i++) {
    if (number % i === 0) return false;
  }
  return true;
});

console.log(arr);

}      
prime();


