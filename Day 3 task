1.JSON iterate over all the loops

FOR:
var arr = [ 1, 2, 3, 4, 5] 
for (var i=0; i < arr.length; i++) {
   console.log(arr[i])
}

FOR IN :
var person = {
   name: "Mohi",
   age: 22
}; 
for (var key in person) {
   console.log(key + ": "+ person[key])
}

FOR EACH:
var color = ["red", "blue", "white"];
color.forEach(clr);
function clr(table, paper) {
   console.log(paper :: table);
}


